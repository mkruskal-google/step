{
  "semi": false,
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2,
        "singleQuote": false,
        "useTabs": false,
      }
    },
    {
      "files": ["*.css"],
      "options": {
        "tabWidth": 2,
        "singleQuote": true,
        "useTabs": false,
      },
    }
  ]
}